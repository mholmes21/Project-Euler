   "source": [
    "loop = 1000\n",
    "c = continued_fraction(sqrt(2))\n",
    "count = 0\n",
    "for i in range(loop):\n",
    "    n = c.numerator(i)\n",
    "    d = c.denominator(i)\n",
    "    if len(str(n)) > len(str(d)):\n",
    "        count += 1\n",
    "        print(\"Another one on line:\", i)\n",
    "\n",
    "\n",
    "print(\"There were\", count, \"special fractions!\")\n"
   ]
